{
  "extends": "makeomatic",
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "env": {
    "es2020": true
  },
  "rules": {
    "prefer-destructuring": ["error", {
      "VariableDeclarator": {
        "array": false,
        "object": true
      },
      "AssignmentExpression": {
        "array": true,
        "object": false
      }
    }, {
      "enforceForRenamedProperties": false
    }],
    "object-curly-newline": 0
  }
}
