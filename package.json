{
  "name": "ms-users",
  "description": "Core of the microservice for handling users",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "rimraf ./lib; babel -d ./lib --copy-files ./src",
    "prepublish": "npm run lint && npm run compile",
    "pretest": "npm run compile",
    "test": "npm run lint && rimraf ./coverage && ./test/docker.sh",
    "start": "node ./bin/mservice.js | bunyan -o short",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "doc": "cd schemas && apidoc -i ./../src/actions -v -o ./../docs",
    "docker-release": "./docker-release.sh",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-users.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-users/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-users#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.8",
    "bytes": "^2.4.0",
    "common-errors": "^1.0.0",
    "csv-write-stream": "^2.0.0",
    "disposable-email-domains": "^1.0.15",
    "dlock": "^6.3.0",
    "flake-idgen": "^1.1.0",
    "get-stdin": "^5.0.1",
    "ioredis": "^2.4.3",
    "is": "^3.2.1",
    "jsonwebtoken": "^7.3.0",
    "jwa": "^1.1.5",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.17.1",
    "ms-amqp-transport": "^7.1.0",
    "ms-conf": "^3.0.4",
    "ms-flakeless": "^4.0.1",
    "ms-mailer-client": "^5.0.1",
    "ms-mailer-templates": "^0.16.0",
    "ms-token": "^1.4.1",
    "ms-validation": "^3.0.0",
    "mservice": "^6.0.1",
    "node-uuid": "^1.4.7",
    "password-generator": "^2.1.0",
    "redis-filtered-sort": "^2.0.6",
    "request": "^2.81.0",
    "request-promise": "^4.1.1",
    "scrypt": "^6.0.1",
    "stdout-stream": "^1.4.0",
    "urlsafe-base64": "^1.0.0",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "apidoc-plugin-schema": "^0.1.1",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-module-method": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "cross-env": "^3.2.3",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.17.1",
    "eslint-config-makeomatic": "^1.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.5.0",
    "faker": "^4.1.0",
    "glob": "^7.1.1",
    "json": "^9.0.6",
    "latest-version-cli": "^1.1.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.2",
    "sinon": "^1.17.7"
  },
  "release": {
    "verifyConditions": []
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  }
}
