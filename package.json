    "node": ">= 8.9.0",
    "npm": ">= 4.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "src/",
    "schemas/",
    "yarn.lock"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
