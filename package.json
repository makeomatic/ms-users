    "prepublishOnly": "npm run compile",
    "semantic-release": "semantic-release",
    "docker-release": "mdep docker release",
    "release": "pnpm i && pnpm run semantic-release",
    "start": "mfleet",
    "prepare": "(mdep install || echo 'skip mdep install')"
