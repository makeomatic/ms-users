{
  "name": "ms-users",
  "version": "0.8.0",
  "description": "Core of the microservice for handling users",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "/bin/bash -c 'if [[ $NODE_ENV != \"production\" ]]; then ./node_modules/.bin/babel -d ./lib --optional es7.objectRestSpread,es7.classProperties,es7.decorators ./src; fi'",
    "prepublish": "npm run compile",
    "pretest": "npm rebuild",
    "test": "BLUEBIRD_DEBUG=1 ./node_modules/.bin/mocha --require ./test/babelhook.js --bail -R spec",
    "start": "node ./bin/mservice.js | ./node_modules/.bin/bunyan -o short"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/ms-users.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-users/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-users#readme",
  "dependencies": {
    "bluebird": "^3.0.5",
    "bunyan": "^1.5.1",
    "bytes": "^2.2.0",
    "common-errors": "^0.5.3",
    "flake-idgen": "^1.0.0",
    "ioredis": "^1.13.0",
    "jsonwebtoken": "^5.4.1",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "ms-amqp-conf": "^0.2.0",
    "ms-amqp-transport": "^0.4.0",
    "ms-mailer-client": "^0.3.0",
    "ms-mailer-templates": "^0.3.0",
    "ms-validation": "^0.5.7",
    "mservice": "^1.0.0",
    "node-uuid": "^1.4.7",
    "password-generator": "^2.0.2",
    "request-promise": "^1.0.2",
    "scrypt": "^5.4.1",
    "urlsafe-base64": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.6",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-mocha": "^1.1.0",
    "faker": "^3.0.1",
    "json": "^9.0.3",
    "mocha": "^2.3.3",
    "mocha-multi": "^0.7.2",
    "server-destroy": "^1.0.1",
    "sinon": "^1.17.2"
  }
