    "handlebars": "^4.0.12",
    "hapi": "^17.6.2",
    "ioredis": "^4.2.0",
    "is": "^3.2.1",
    "jsonwebtoken": "^8.3.0",
    "jwa": "^1.1.6",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ms-conf": "^3.3.1",
    "ms-flakeless": "^4.1.0",
