{
  "name": "ms-users",
  "description": "Core of the microservice for handling users",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "rimraf ./lib; babel -d ./lib --copy-files ./src",
    "pretest": "npm run compile",
    "test": "npm run lint && npm run test:integration -- -r flake-idgen -r ms-flakeless -r scrypt",
    "test:integration": "mkdir -p ./ss && mdep test run",
    "lint": "eslint ./src",
    "doc": "cd ./schemas && apidoc -i ../src/actions -v --debug -o ../docs",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docker-release": "mdep docker release --node 8.1.0",
    "release": "yarn; npm run semantic-release; ./scripts/release.sh",
    "start": "node ./bin/mservice.js | bunyan -o short"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-users.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-users/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-users#readme",
  "dependencies": {
    "@microfleet/core": "^9.1.0",
    "@microfleet/transport-amqp": "^9.1.1",
    "babel-runtime": "^6.23.0",
    "bell": "^8.5.0",
