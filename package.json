  "scripts": {
    "compile": "rimraf ./lib; cp -r ./src ./lib",
    "pretest": "npm run compile",
    "test": "npm run lint && npm run test:e2e",
    "test:e2e:cluster": "DB_SRV=redisCluster mdep test run",
    "test:e2e:sentinel": "DB_SRV=redisSentinel mdep test run",
