{
  "name": "ms-users",
  "description": "Core of the microservice for handling users",
  "main": "./lib/index.js",
  "version": "0.0.0-development",
  "scripts": {
    "compile": "rimraf ./lib; babel -d ./lib --copy-files ./src",
    "pretest": "npm run compile",
    "test": "npm run lint && npm run test:e2e -- -r flake-idgen -r ms-flakeless -r scrypt",
    "test:e2e": "mkdir -p ./ss && mdep test run",
    "lint": "eslint ./src",
    "doc": "cd ./schemas && apidoc -i ../src/actions -v --debug -o ../docs",
    "prepublishOnly": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docker-release": "mdep docker release",
    "release": "yarn && npm run semantic-release && ./scripts/release.sh",
    "start": "mfleet",
    "commit": "simple-commit-message"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-users.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-users/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-users#readme",
  "dependencies": {
    "@microfleet/core": "^9.7.5",
    "@microfleet/transport-amqp": "^12.2.1",
    "babel-runtime": "^6.25.0",
    "bell": "^8.5.0",
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.12",
    "bytes": "^3.0.0",
    "common-errors": "^1.0.4",
    "csv-write-stream": "^2.0.0",
    "disposable-email-domains": "^1.0.20",
    "dlock": "^6.4.0",
    "flake-idgen": "^1.1.0",
    "get-stdin": "^5.0.1",
    "handlebars": "^4.0.6",
    "hapi": "^16.5.2",
    "ioredis": "^3.1.2",
    "is": "^3.2.1",
    "jsonwebtoken": "^8.0.1",
    "jwa": "^1.1.5",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "ms-conf": "^3.1.2",
    "ms-flakeless": "^4.0.1",
    "ms-mailer-client": "^6.0.2",
    "ms-mailer-templates": "^1.1.0",
    "ms-token": "^2.0.0",
    "ms-validation": "^6.0.2",
    "node-uuid": "^1.4.8",
    "password-generator": "^2.1.0",
    "redis-filtered-sort": "^2.2.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "scrypt": "^6.0.1",
    "serialize-error": "^2.1.0",
    "serialize-javascript": "^1.4.0",
    "stdout-stream": "^1.4.0",
    "tough-cookie": "^2.3.2",
    "vision": "^4.1.1",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@makeomatic/deploy": "^4.1.7",
    "apidoc": "^0.17.6",
    "apidoc-plugin-schema": "^0.1.6",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.1",
    "cheerio": "^1.0.0-rc.2",
    "codecov": "^2.3.0",
    "cross-env": "^5.0.3",
    "eslint": "^4.4.0",
    "eslint-config-makeomatic": "^1.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-promise": "^3.5.0",
    "faker": "^4.1.0",
    "glob": "^7.1.2",
    "json": "^9.0.6",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "semantic-release": "^8.0.3",
    "sinon": "^4.0.0"
  },
  "release": {
    "verifyConditions": "@makeomatic/condition-semaphore",
    "analyzeCommits": "simple-commit-message",
    "generateNotes": "github-post-release",
    "getLastRelease": "@makeomatic/last-release-npm",
    "branch": "master"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 4.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "src/",
    "yarn.lock"
  ]
}
