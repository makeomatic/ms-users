{
  "$id": "auth-bypass",
  "description": "authenticate through 3rd party api",
  "required": ["schema", "userKey"],
  "properties": {
    "schema": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9\\-_\\.]+:[a-zA-Z0-9\\-_\\.]+$"
    },
    "userKey": {
      "type": "string",
      "minLength": 1
    },
    "init": {
      "type": "boolean"
    }
  }
}
