{
    "$id": "mfa.regenerate-codes",
    "type": "object",
    "required": [
        "username"
    ],
    "properties": {
      "username": {
          "type": "string",
          "minLength": 1
      },
        "totp": {
            "type": "string",
            "minLength": 6,
            "maxLength": 36
        },
        "remoteip": {
            "type": "string",
            "format": "ipv4"
        }
    }
}
