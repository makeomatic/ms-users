{
  "$id": "oauth.apple-code",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "state": {
      "description": "Bell state created on init sign in",
      "type": "string",
      "maxLength": 512
    },
    "code": {
      "description": "Apple auth code",
      "type": "string",
      "maxLength": 512
    },
    "user": {
      "description": "JSON with user info from Apple",
      "type": "string",
      "maxLength": 2048
    }
  }
}
