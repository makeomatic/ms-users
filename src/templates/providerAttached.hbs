<!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{{ title }}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body>
    <script>
      var opener = window.opener;
      var $ms_users_inj_post_message = {{{ message }}};
      if (opener) {
        opener.postMessage($ms_users_inj_post_message, '{{ targetOrigin }}');
      }
      // close the window
      window.close();
    </script>
  </body>
</html>
